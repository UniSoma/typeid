#kaocha/v1
 {:tests [{:id :unit
           :test-paths ["test"]
           :source-paths ["src"]
           :ns-patterns [".*-test$"]}]
  :plugins [:kaocha.plugin/profiling
            :kaocha.plugin/capture-output]
  :color? true
  :randomize? true
  :fail-fast? false
  :reporter [kaocha.report/documentation]}
