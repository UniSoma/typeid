#kaocha/v1
 {:tests
  [{:id :unit-cljs
    :type :kaocha.type/cljs
    :test-paths ["test"]
    :source-paths ["src"]
    :cljs/timeout 10000
    :cljs/repl-env cljs.repl.node/repl-env
    :cljs/compiler-options {:warnings {:undeclared-var {:exclude-from-regex "goog\\.math\\.Long"}}}}]

  :color? true
  :fail-fast? false
  :randomize? true
  :reporter kaocha.report.progress/report
  :plugins [:kaocha.plugin/profiling :kaocha.plugin/capture-output]}

